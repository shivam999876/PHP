<?php
$x=2;
echo ++$x, " First increments then prints ";
echo $x;
echo"<br>";

$x=2;
echo $x++, " First prints then increments ";
echo $x;
echo"<br>";

$x=2;
echo --$x, " First decrements then prints ";
echo $x;
echo"<br>";

$x=2;
echo $x--, " First prints then decrements ";
echo $x;
echo"<br>";

?>
